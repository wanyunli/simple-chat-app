    "babel-polyfill": "^6.26.0",
    "dotenv-webpack": "^1.5.7",
    "express": "^4.16.4",
    "mongoose": "^5.3.15",
    "prop-types": "^15.6.2",
    "ramda": "^0.26.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
